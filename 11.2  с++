#include <iostream>

int main() {
    int arr[7];
    std::cout << "Введите 7 целых чисел:\n";

    for (int i = 0; i < 7; i++) {
        std::cout << "Элемент " << i << ": ";
        std::cin >> arr[i];
    }

    std::cout << "\nМассив заполнен следующими значениями:\n";

    for (int i = 0; i < 7; i++) {
        std::cout << arr[i] << " ";
    }

    std::cout << std::endl;

    return 0;
}
