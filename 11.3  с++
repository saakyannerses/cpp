#include <iostream>
#include <cstdlib>
#include <ctime>

int main() {
    // Инициализация генератора случайных чисел
    srand(time(NULL));

    // a) Вещественные значения в диапазоне от 0 до 1
    float arr_a[10];
    for (int i = 0; i < 10; i++) {
        arr_a[i] = static_cast<float>(rand()) / RAND_MAX;
    }

    // б) Вещественные значения x (22 < x < 23)
    float arr_b[10];
    for (int i = 0; i < 10; i++) {
        arr_b[i] = 22.0 + static_cast<float>(rand()) / (RAND_MAX / 1.0);
    }

    // в) Вещественные значения x (0 <= x < 10)
    float arr_c[10];
    for (int i = 0; i < 10; i++) {
        arr_c[i] = static_cast<float>(rand()) / (RAND_MAX / 10.0);
    }

    // г) Вещественные значения x (-50 <= x < 50)
    float arr_g[10];
    for (int i = 0; i < 10; i++) {
        arr_g[i] = -50.0 + static_cast<float>(rand()) / (RAND_MAX / 100.0);
    }

    // д) Целые значения в диапазоне от 1 до 10 включительно
    int arr_d[10];
    for (int i = 0; i < 10; i++) {
        arr_d[i] = rand() % 10 + 1;
    }

    // Вывод заполненных массивов
    std::cout << "a) Вещественные значения в диапазоне от 0 до 1:" << std::endl;
    for (int i = 0; i < 10; i++) {
        std::cout << arr_a[i] << " ";
    }
    std::cout << std::endl << std::endl;

    std::cout << "б) Вещественные значения x (22 < x < 23):" << std::endl;
    for (int i = 0; i < 10; i++) {
        std::cout << arr_b[i] << " ";
    }
    std::cout << std::endl << std::endl;

    std::cout << "в) Вещественные значения x (0 <= x < 10):" << std::endl;
    for (int i = 0; i < 10; i++) {
        std::cout << arr_c[i] << " ";
    }
    std::cout << std::endl << std::endl;

    std::cout << "г) Вещественные значения x (-50 <= x < 50):" << std::endl;
    for (int i = 0; i < 10; i++) {
        std::cout << arr_g[i] << " ";
    }
    std::cout << std::endl << std::endl;

    std::cout << "д) Целые значения в диапазоне от 1 до 10 включительно:" << std::endl;
    for (int i = 0; i < 10; i++) {
        std::cout << arr_d[i] << " ";
    }
    std::cout << std::endl;

    return 0;
}
