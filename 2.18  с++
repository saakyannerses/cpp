#include <iostream>
#include <cmath>
using namespace std;
int main() {
    float x, y, z, q;

    // Ввод значений x и y
    cout << "Введите значение x: ";
    cin >> x;
    cout << "Введите значение y: ";
    cin >> y;

    // Вычисление значения z
    z = (x + (2 + y) / (x * x)) / (y + 1 / sqrt(x * x + 10));

    // Вычисление значения q
    q = 7.2 * sin(x) - fabs(y);

    // Вывод результатов
    cout << "Значение z: " << z << endl;
    cout << "Значение q: " << q << endl;

    return 0;
}
